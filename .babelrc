{
    "presets": [
        "flow"
    ],
    "env": {
        "production": {
            "presets": [
                ["es2015", {
                    "modules": false
                }]
            ],
            "plugins": [
                "transform-class-properties",
                "transform-runtime"
            ]
        },
        "development": {
            "presets": [
                ["es2015", {
                    "modules": false
                }]
            ],
            "plugins": [
                "transform-class-properties",
                "transform-runtime"
            ]
        },
        "test": {
            "presets": [
                "es2015"
            ],
            "plugins": [
                "transform-class-properties",
                "transform-runtime"
            ]
        },
        "esm": {
            "plugins": [
                "transform-class-properties",
                "./contrib/babel-plugin-esm/babel-plugin-esm.js"
            ]
        }
    }
}
