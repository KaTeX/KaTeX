    "babel-preset-es2015": "^6.18.0",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.24.0",
    "babelify": "^7.3.0",
    "browserify": "^13.3.0",
    "check-dependencies": "^1.1.0",
    "clean-css": "^3.4.23",
    "eslint": "^4.6.1",
    "eslint-plugin-flowtype": "^2.35.1",
    "express": "^4.14.0",
    "flow-bin": "^0.54.0",
    "glob": "^7.1.1",
    "jest": "^20.0.4",
    "jest-serializer-html": "^4.0.0",
    "js-yaml": "^3.3.1",
    "jspngopt": "^0.2.0",
